<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.rockshassa.NicksPlugin" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0">
    <name>nicksPlugin</name>
    <description></description>
    <license>MIT</license>
    <keywords></keywords>
<!--    <repo>https://github.com/rockshassa/nicksPlugin.git</repo>-->
    <issue>https://github.com/rockshassa/nicksPlugin</issue>

    <!-- ios -->
    <js-module src="www/plugin.js" name="plugin">
        <runs/>
        <clobbers target="NicksPlugin" />
    </js-module>
    <config-file target="config.xml" parent="/*">
        <feature name="NicksPlugin">
            <param name="ios-package" value="NicksPlugin"/>
        </feature>
    </config-file>
    <header-file src="src/ios/NicksPlugin.h" />
    <source-file src="src/ios/NicksPlugin.m" />
</plugin>
